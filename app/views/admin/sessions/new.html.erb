<div class="page-header">
  <h1><%= controller_name.humanize%> <smal><%=action_name %></smal></h1>
</div>


<div class="body_panel panel-default">
<div class="panel-body">

  <%= form_for :session, url: admin_sessions_path, html: {class: 'form-horizontal'} do |f| %>
  <div class="field form-group">
    <%= f.label :email, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= text_field(:sessions, :email, class: 'col-sm-2') %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :password, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= password_field(:sessions, :password, class: 'form-control') %>
    </div>
  </div>



      <div class="actions form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <%= f.submit "Login"%>
        </div>
      </div>

      </div>


</div>


<% end %>